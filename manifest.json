{ "manifest_version": 2, 
    "name": "Bruh", 
    "description": "We will be creating a chrome extension that can save URL links, and highlight parts of a webpage and save it.", 
    "version": "1", 
    "authors": "Max Bromet and Eileen Lee", 
    "browser_action": {  
        "default_title": "Bruh", 
        "default_icon": "images/snek.png",
        "default_popup": "popup.html"
    },

    "web_accessible_resources" : [
        "images/doge.png"
    ],
    "permissions": ["tabs", "activeTab", "declarativeContent", "storage", "https://some-random-api.ml/*"],
    
    "background" : {
        "scripts" : ["background.js"],
        "persistent": false
    },

    "content_scripts" : [{
        "matches" : ["<all_urls>"],
        "all_frames": true,
        "js" : ["content.js"]
    }]
    
    
}
