{ "manifest_version": 1, "name": "PageSaver?", 
    "description": "We will be creating a chrome extension that can save URL links, and highlight parts of a webpage and save it.", 
    "version": "1", "authors": "Max Bromet and Eileen Lee", 
    "browser_action": {  "default_popup": "popup.html",
        "default_icon": "icon.png", 
        "default_title": "Page Saver" 
    },

    "chrome_url_overrides" : {  "newtab": "newtab.html"},
    "permissions": ["activeTab"]}
